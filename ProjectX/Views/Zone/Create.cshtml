@model ProjectX.Entities.Models.Zone.ZoneGetResp
@{
    //var filldata = ViewData["filldata"] as ProjectX.Entities.Models.General.LoadDataResp;
}


<div class="content" pgtype="view">
    <div class="container-fluid" style="padding:10px 0">
        <div class="row mb-3">
            <div class="col-md-12 ">
                <div class="card h-100 ">
                    <div class="card-body">
                        <h4 class="card-title ">
                            <i class="fa fa-arrow-alt-circle-left pointercursor" onclick="history.back()"></i>
                             <span>New Zone </span>
                        </h4>
                        <hr />
                        <div class="row">
                            <div id="divinfo" class="col-lg-12">
                                <div class="row">
                                    <div class="form-group mb-3 col-lg-4">
                                        <label for="Z_Id">Z_Id</label>
                                        <input type="number" class="form-control" id="Z_Id" name="Z_Id">
                                    </div>

                                    <div class="form-group mb-3 col-lg-4">
                                        <label for="Z_Title">Z_Title</label>
                                        <input type="text" class="form-control" id="Z_Title" name="Z_Title" required="">
                                    </div>

                                    <div class="form-group mb-3 col-lg-4">
                                        <label for="Z_Destination_Id">Z_Destination_Id</label>
                                        <select class="form-control" id="Z_Destination_Id" name="Z_Destination_Id" multiple>
                                                <option  selected>item</option>
                                        </select>
                                    </div>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card card-body">
                    <div class="row">
                        <div class="col-lg-8">
                        </div>
                        <div class="col-lg-4">
                            <div class="row">
                                <div class="col-lg-8">
                                </div>
                                <div class="col-lg-4">
                                    <button id="create" class="btn btn-primary w-100 bglocalcolor" type="submit">
                                        <span> Add </span>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






