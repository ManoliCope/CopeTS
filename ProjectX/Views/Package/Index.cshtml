@model ProjectX.Entities.Models.General.LoadDataResp


<div class="content">
    <!-- Start Content-->
    <div class="row page-title">
        <div class="col-md-12">
            @*<span class="pointercursor">
                    <a href="~/package/create/">
                        <i class="fas fa-plus-circle addnew pointercursor float-right"></i>
                    </a>
                </span>*@
            <a class="btn bglocalcolor float-right addnewbtn" href="~/package/create/">
                <i class="fas fa-plus-circle"></i> Add
            </a>
            <h4 class="mb-0 mt-0 float-left">Packages</h4>

        </div>
    </div>
    <div class="container-fluid " style="padding:10px 0">
        <div class="row">
            <div class="col-lg-12">
				<div class="card">
					<div id="searchform" class="card-body">
						<div>
							<div class="row mb-6">
								@*<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="Id">ID:</label>
									<input id="Id" type="text" class="form-control datatable-input" placeholder="Enter ID" data-col-index="0">
								</div>*@
								<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="Name">Name:</label>
									<input id="Name" type="text" class="form-control datatable-input" placeholder="Enter Name" data-col-index="1">
								</div>
								<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="ProductId">Product:</label>
									<select id="ProductId" class="form-control datatable-input isselect2">
										<option value="">Select Product</option>
										@foreach (var thislist in Model.loadedData.products)
										{
											<option value="@thislist.LK_ID"> @thislist.LK_TableField</option>
										}
									</select>
								</div>
								<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="ZoneID">Zone:</label>
									<select id="ZoneID" class="form-control datatable-input isselect2">
										<option value="">Select Zone</option>
										@foreach (var thislist in Model.loadedData.zones)
										{
											<option value="@thislist.LK_ID"> @thislist.LK_TableField</option>
										}
									</select>
								</div>
								@*<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="Remove_deductable">Remove Deductible:</label>
									<input id="Remove_deductable" type="text" class="form-control datatable-input" placeholder="Enter Remove Deductible" data-col-index="4">
								</div>
								<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="Adult_No">Adult Number:</label>
									<input id="Adult_No" type="text" class="form-control datatable-input" placeholder="Enter Adult Number" data-col-index="5">
								</div>
								<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="Children_No">Children Number:</label>
									<input id="Children_No" type="text" class="form-control datatable-input" placeholder="Enter Children Number" data-col-index="6">
								</div>*@
								@*<div class="col-lg-3 mb-lg-0 mb-6">
									<label for="PA_Included">PA Included:</label>
									<input id="PA_Included" type="checkbox" class="form-control datatable-input" data-col-index="7">
								</div>*@

								<div class="col-lg-9">
								</div>
								<div class="col-lg-3">
									<div class="row mt-4 pt-1">
										<div class="col-sm-6">
											<button class="btn bglocalcolor" id="search" style="width:100%">
												<span>
													<i class="la la-search"></i>
													<span>Search</span>
												</span>
											</button>&nbsp;&nbsp;
										</div>
										<div class="col-sm-6">
											<button class="btn btn-info resetdiv" id="kt_reset" style="width:100%">
												<span>
													<i class="la la-close"></i>
													<span>Reset</span>
												</span>
											</button>
										</div>
									</div>
								</div>

							</div>
						</div>
					</div>

				</div>
            </div>
        </div>

        <div class="row tablediv">
            <div class="col-lg-12">
                <div class="card">
                    <div class="card-body">
                        <table id="packagetable" class="table dt-responsive nowrap table compact table-hover table-flush table-striped table-bordered" style="width:100%; ">
                        </table>

                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

<div id="confirm-email-approval" class="modal fade" data-backdrop="static" data-keyboard="false">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="exampleModalLongTitle">Delete confirmation</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                Are you sure you want to delete this record?
            </div>
            <div class="modal-footer">
                <button type="button" class="btn  bglocalcolor border" data-dismiss="modal">Close</button>
                <div id="thisloader" class="smloader hide"></div>
                <button id="confirmdeletebtn"  type="button" class="btn btn-danger">Proceed</button>
            </div>
        </div>
    </div>
</div>
