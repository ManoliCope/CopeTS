@model ProjectX.Entities.Models.Package.PackGetResp
@{
    var filldata = ViewData["filldata"] as ProjectX.Entities.Models.General.LoadDataResp;
}


<div class="content" pgtype="view">
    <div class="container-fluid" style="padding:10px 0">
        <div class="row mb-3">
            <div class="col-md-12 ">
                <div class="card h-100 ">
                    <div class="card-body">
                        <h4 class="card-title ">
                            <i class="fa fa-arrow-alt-circle-left pointercursor" onclick="history.back()"></i>
                             <span>New </span>
                        </h4>
                        <hr />
                        <div class="row">
                            <div id="divinfo" class="col-lg-12">
                                <div class="row">
                                    <div class="form-group">
                                        <label for="id">ID</label>
                                        <input type="number" class="form-control" id="id" name="id" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="name">Name</label>
                                        <input type="text" class="form-control" id="name" name="name" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="cob">COB</label>
                                        <input type="text" class="form-control" id="cob" name="cob" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="cobId">COB ID</label>
                                        <input type="number" class="form-control" id="cobId" name="cobId" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="plan">Plan</label>
                                        <input type="text" class="form-control" id="plan" name="plan" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="planId">Plan ID</label>
                                        <input type="number" class="form-control" id="planId" name="planId" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="zone">Zone</label>
                                        <input type="text" class="form-control" id="zone" name="zone" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="zoneId">Zone ID</label>
                                        <input type="number" class="form-control" id="zoneId" name="zoneId" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="remove_deductable">Remove Deductible</label>
                                        <input type="number" class="form-control" id="remove_deductable" name="remove_deductable" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="adult_no">Adult Count</label>
                                        <input type="number" class="form-control" id="adult_no" name="adult_no" required>
                                    </div>

                                    <div class="form-group">
                                        <label for="children_no">Children Count</label>
                                        <input type="number" class="form-control" id="children_no" name="children_no" required>
                                    </div>

                                    <div class="form-group">
                                        <div class="form-check">
                                            <input class="form-check-input" type="checkbox" id="pa_included" name="pa_included">
                                            <label class="form-check-label" for="pa_included">
                                                PA Included
                                            </label>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="card card-body">
                    <div class="row">
                        <div class="col-lg-8">
                        </div>
                        <div class="col-lg-4">
                            <div class="row">
                                <div class="col-lg-8">
                                </div>
                                <div class="col-lg-4">
                                    <button id="create" class="btn btn-primary w-100 bglocalcolor" type="submit">
                                        <span> Add </span>
                                    </button>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>






