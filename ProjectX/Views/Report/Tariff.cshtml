@model ProjectX.Entities.Models.General.LoadDataResp
<link href="~/Content/assets/css/registercall.css" rel="stylesheet" asp-append-version="true" />

<div class="content">
    <div class="container-fluid" style="padding:10px 0">
        <div class="row ">
            <div id="calldiv" class="col-lg-12  mb-3">
                <div class="card h-100 mb-0 pb-1">
                    <div class="card-body card-body pb-0">
                        <h4 class="card-title ">
                            <i class="fa fa-arrow-alt-circle-left pointercursor" onclick="history.back()"></i>
                            Tariff Report
                        </h4>
                        <hr/>
                        <div class="row">
                            <div class="col-lg-12">
                                <div class="card m-0" style="box-shadow:none !important">
                                    <div id="searchform" class="card-body ">
                                        <div class="row mb-6">
                                               <div class="col-md-3 mb-lg-0 mb-6">
                                                <label>Users:</label>
                                                 <select class="form-control" name="userId" id="assignedUserId" field-name="user" required >
                                 <option value=""> Select User</option>
                                 @foreach (var thislist in Model.loadedData.assignedUsers)
											        {
											        	<option prid="@thislist.LK_ParentID" value="@thislist.LK_ID"> @thislist.LK_TableField</option>
											        }
                                

                            </select>
                                            </div>
                                      
                                            
                                               <div class="col-md-3 mb-lg-0 mb-6">
                                                <label>Products:</label>
                                                 <select class="form-control" name="productId" id="tproductId" field-name="product" required >
                                 <option value=""> Select Product</option>
                                 @foreach (var thislist in Model.loadedData.products)
											        {
											        	<option value="@thislist.LK_ID"> @thislist.LK_TableField</option>
											        }
                                

                            </select>
                                            </div>
                                                  <div class="col-md-3 mb-lg-0 mb-6">
                                                <label>Packages:</label>
                                                 <select class="form-control" name="packageId" id="tpackageId" field-name="package" required >
                                 <option value=""> Select Package</option>
                                 @foreach (var thislist in Model.loadedData.packages)
											        {
											        	<option prid="@thislist.LK_ParentID" value="@thislist.LK_ID"> @thislist.LK_TableField</option>
											        }
                                

                            </select>
                                            </div> 
                                             <div class="col-md-3 mb-lg-0 mb-6">
                                                <label>Plans:</label>
                                                 <select class="form-control" name="planId" id="tplanId" field-name="plan" required >
                                 <option value=""> Select Plan</option>
                                 @foreach (var thislist in Model.loadedData.plans)
											        {
											        	<option value="@thislist.LK_ID"> @thislist.LK_TableField</option>
											        }
                                

                            </select>
                                            </div>
                                        </div>

                                        <div class="row mb-6">
                                          
                                           
                                         
                                            <div class="col-md-3 mt-1">
                                                <div class="row mt-4">
                                                    <div class="col-md-12">
                                                        <button class="btn bglocalcolor loadingthis" id="generatetariff" data-toggle="tooltip"
                                                                data-original-title="Check dates!" style="width:100%">
                                                            <span>
                                                                <i class="la la-search"></i>
                                                                <span>Generate</span>
                                                            </span>
                                                        </button>&nbsp;&nbsp;

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="row mb-8">
                                            <div class="col-lg-9 mb-lg-0 mb-6">
                                            </div>
                                            <div class="col-lg-3">

                                            </div>

                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>


