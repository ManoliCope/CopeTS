@model ProjectX.Entities.Models.Users.UsersResp
@{
	var filldata = ViewData["loadDataCreate"] as ProjectX.Entities.Models.General.LoadDataResp;
	//var userId= ViewData["userid"] as string;
}



<div class="content" pgtype="view">
	<div class="container-fluid" style="padding:10px 0">
		<div class="card-body">
			<form class="needs-validation" novalidate="" id="addUserForm" userid="@Model.id" autocomplete="off">
				<div class="row">
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="firstName">First Name *</label>
							<input type="text" class="form-control" id="firstName" field-name="first_Name" required value="@Model.first_Name">
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="middleName">Middle Name</label>
							<input type="text" class="form-control" id="middleName" field-name="middle_Name" value="@Model.middle_Name" required>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="lastName">Last Name *</label>
							<input type="text" class="form-control" id="lastName" field-name="last_Name" value="@Model.last_Name" required>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="userName">User Name *</label>
							<input type="text" class="form-control" id="userName" field-name="user_Name" value="@Model.user_Name" required>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="categoryId">Category *</label>
							<select class="form-control" name="categoryId" id="categoryId" field-name="category" required onchange="ChangeFormByCategoryId()">
								<option></option>
								@foreach (var thislist in filldata.loadedData.userCategory)
								{

									@if (thislist.LK_ID == Convert.ToInt64(Model.category))
									{
										<option value="@thislist.LK_ID" selected> @thislist.LK_TableField</option>
									}
									else
									{
										<option value="@thislist.LK_ID"> @thislist.LK_TableField</option>
									}
								}


							</select>



						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="lastName">Broker Code *</label>
							<input type="text" class="form-control" id="brkNumber" value="@Model.broker_Code" field-name="broker_Code" required>
						</div>
					</div>

					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="country">Country *</label>
							<select class="form-control" name="countryId" id="countryId" field-name="country" required>
								<option></option>
								@foreach (var thislist in filldata.loadedData.destinations)
								{

									@if (thislist.D_Id == Model.country)
									{
										<option value="@thislist.D_Id" selected> @thislist.D_Destination</option>
									}
									else
									{
										<option value="@thislist.D_Id"> @thislist.D_Destination</option>
									}
								}

							</select>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="city">City</label>
							<input type="text" class="form-control" id="city" field-name="city" name="city" value="@Model.city" required>
						</div>
					</div>

					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="telephone">Telephone *</label>
							<div class="input-group">
								<input type="text" class="form-control" field-name="country_code" maxlength="3" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*?)\..*/g, '$1');" id="ccode" name="ccode" value="@Model.country_code">
								<input type="text" class="form-control" field-name="telephone" maxlength="12" oninput="this.value = this.value.replace(/[^0-9.]/g, '').replace(/(\..*?)\..*/g, '$1');" id="telephone" name="telephone" value="@Model.telephone">
							</div>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="email">Email *</label>
							<input type="email" class="form-control" field-name="email" id="email" name="email" value="@Model.email" required>
						</div>
					</div>
					<div class="col-md-3">
						<div class="form-group">
							<label class="form-control-label" for="superAgentId">Super-Agent *</label>
